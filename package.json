{
  "name": "swust-timetable-getter",
  "version": "1.0.0",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "npx ts-node -r tsconfig-paths/register --files ./src --env=development"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@util": "dist/util",
    "@models": "dist/models"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0-rc.12",
    "command-line-args": "^5.2.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "got-cjs": "^12.3.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.2.6",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.3.0",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "jasmine": "^4.4.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
